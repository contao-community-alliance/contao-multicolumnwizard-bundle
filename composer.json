    {
      "name": "Andreas Schempp",
      "email": "andreas.schempp@terminal42.ch",
      "homepage": "http://www.terminal42.ch",
      "role": "Developer"
    },
    {
      "name": "Yanick Witschi",
      "email": "yanick.witschi@terminal42.ch",
